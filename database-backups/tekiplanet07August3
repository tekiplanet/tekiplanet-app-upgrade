--
-- Table structure for table `business_invoices`
--

DROP TABLE IF EXISTS `business_invoices`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `business_invoices` (
  `id` char(36) NOT NULL,
  `business_id` char(36) NOT NULL,
  `customer_id` char(36) NOT NULL,
  `invoice_number` varchar(255) NOT NULL,
  `amount` decimal(15,2) NOT NULL,
  `currency` varchar(3) NOT NULL DEFAULT 'NGN',
  `paid_amount` decimal(15,2) NOT NULL DEFAULT 0.00,
  `due_date` date NOT NULL,
  `status` enum('draft','pending','sent','partially_paid','paid','overdue','cancelled') NOT NULL DEFAULT 'draft',
  `payment_reminder_sent` tinyint(1) NOT NULL DEFAULT 0,
  `theme_color` varchar(255) DEFAULT NULL,
  `notes` text DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `business_invoices_invoice_number_unique` (`invoice_number`),
  KEY `business_invoices_business_id_foreign` (`business_id`),
  KEY `business_invoices_customer_id_foreign` (`customer_id`),
  CONSTRAINT `business_invoices_business_id_foreign` FOREIGN KEY (`business_id`) REFERENCES `business_profiles` (`id`) ON DELETE CASCADE,
  CONSTRAINT `business_invoices_customer_id_foreign` FOREIGN KEY (`customer_id`) REFERENCES `business_customers` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

